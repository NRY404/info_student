{"ast":null,"code":"import \"core-js/modules/es.function.name.js\";\nimport \"core-js/modules/es.array.map.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: \"File\",\n  data: function data() {\n    return {\n      tableData: [],\n      multipleSelection: [],\n      pageNum: 1,\n      pageSize: 5,\n      total: 0,\n      name: '',\n      isDelete: false,\n      headerBg: 'headerBg'\n    };\n  },\n  mounted: function mounted() {\n    this.load();\n  },\n  methods: {\n    load: function load() {\n      var _this = this;\n\n      this.request.get(\"/file/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name,\n          isDelete: this.isDelete\n        }\n      }).then(function (req) {\n        _this.tableData = req.data.records;\n        _this.total = req.data.total;\n      });\n    },\n    //复选框选中后执行的方法\n    handleSelectionChange: function handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //分页\n    handleSizeChange: function handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n\n      if (this.username == null && this.email == null && this.address == null) {\n        this.load();\n      } else {\n        this.selectByCondition();\n      }\n    },\n    handleCurrentChange: function handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.selectByCondition();\n    },\n    //表格数据操作\n    handleEdit: function handleEdit(row) {\n      // this.form = row;\n      this.form = Object.assign({}, row); //将row拷贝到空对象中，解决没点确定 表中数据改变的问题\n\n      this.dialogVisible = true;\n      this.formTitle = '修改用户信息';\n    },\n    handleFileUploadSuccess: function handleFileUploadSuccess() {\n      this.load();\n    },\n    //重置\n    reset: function reset() {\n      this.name = '';\n      this.load();\n    },\n    //文件下载\n    download: function download(url) {\n      window.open(url);\n    },\n    //设置 状态\n    changeEnable: function changeEnable(row) {\n      var _this2 = this;\n\n      this.request.post(\"/file/enable\", row).then(function (res) {\n        if (res.code === '200') {\n          _this2.$message.success(\"操作成功\");\n        } else {\n          _this2.$message.error(\"操作失败\");\n        }\n      });\n    },\n    //数据删除\n    handleDelete: function handleDelete(id) {\n      var _this3 = this;\n\n      this.request(\"/file/delete/\" + id).then(function (resp) {\n        if (resp.code === '200') {\n          //    删除成功\n          _this3.$message.success(\"删除成功\");\n\n          _this3.load();\n        } else {\n          _this3.$message.error(\"删除失败\");\n        }\n      });\n    },\n    //批量删除\n    deleteByIds: function deleteByIds() {\n      var _this4 = this;\n\n      // 将对象类型 直接转为 id数组\n      // [{},{},{}] => {1,2,3}\n      var ids = this.multipleSelection.map(function (v) {\n        return v.id;\n      });\n      this.request.post(\"/file/deletes\", ids).then(function (resp) {\n        if (resp) {\n          //    删除成功\n          _this4.load();\n\n          _this4.$message({\n            message: '批量删除成功!',\n            type: 'success'\n          });\n        } else {\n          _this4.$message.error(\"删除失败\");\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA6FA;AACAA,cADA;AAEAC,MAFA,kBAEA;AACA;AACAC,mBADA;AAEAC,2BAFA;AAGAC,gBAHA;AAIAC,iBAJA;AAKAC,cALA;AAMAN,cANA;AAOAO,qBAPA;AAQAC;AARA;AAUA,GAbA;AAcAC,SAdA,qBAcA;AACA;AACA,GAhBA;AAkBAC;AAEAC,QAFA,kBAEA;AAAA;;AACA;AACAC;AACAR,+BADA;AAEAC,iCAFA;AAGAL,yBAHA;AAIAO;AAJA;AADA,SAOAM,IAPA,CAOA;AACA;AACA;AAEA,OAXA;AAaA,KAhBA;AAiBA;AACAC,yBAlBA,iCAkBAC,GAlBA,EAkBA;AACA;AACAC;AAEA,KAtBA;AAuBA;AACAC,oBAxBA,4BAwBAZ,QAxBA,EAwBA;AACA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;AAEA,KAhCA;AAiCAa,uBAjCA,+BAiCAd,OAjCA,EAiCA;AACA;AACA;AACA,KApCA;AAsCA;AACAe,cAvCA,sBAuCAC,GAvCA,EAuCA;AACA;AACA,yCAFA,CAEA;;AACA;AACA;AACA,KA5CA;AA6CAC,2BA7CA,qCA6CA;AACA;AACA,KA/CA;AAgDA;AACAC,SAjDA,mBAiDA;AACA;AACA;AACA,KApDA;AAqDA;AACAC,YAtDA,oBAsDAC,GAtDA,EAsDA;AACAC;AACA,KAxDA;AA0DA;AACAC,gBA3DA,wBA2DAN,GA3DA,EA2DA;AAAA;;AACA;AACA;AACA;AAEA,SAHA,MAGA;AACA;AACA;AACA,OAPA;AAQA,KApEA;AAsEA;AACAO,gBAvEA,wBAuEAC,EAvEA,EAuEA;AAAA;;AACA;AACA;AACA;AACA;;AACA;AACA,SAJA,MAIA;AACA;AACA;AACA,OARA;AASA,KAjFA;AAmFA;AACAC,eApFA,yBAoFA;AAAA;;AACA;AACA;AACA;AAAA;AAAA;AACA;AACA;AACA;AACA;;AACA;AACAC,8BADA;AAEAC;AAFA;AAIA,SAPA,MAOA;AACA;AACA;AACA,OAXA;AAYA;AApGA;AAlBA","names":["name","data","tableData","multipleSelection","pageNum","pageSize","total","isDelete","headerBg","mounted","methods","load","params","then","handleSelectionChange","val","console","handleSizeChange","handleCurrentChange","handleEdit","row","handleFileUploadSuccess","reset","download","url","window","changeEnable","handleDelete","id","deleteByIds","message","type"],"sourceRoot":"src/views","sources":["File.vue"],"sourcesContent":["<template>\r\n    <div>\r\n<!--        功能区 上传文件 批量删除-->\r\n        <div style=\"margin: 10px 0\">\r\n            <el-upload action=\"http://localhost:8081/file/upload\"\r\n                       :show-file-list=\"false\"\r\n                       :on-success=\"handleFileUploadSuccess\"\r\n                       style=\"display: inline-block;margin-left: 5px;\">\r\n                <el-button type=\"info\" round>上传文件<i class=\"el-icon-upload\"></i></el-button>\r\n            </el-upload>\r\n            <el-popconfirm\r\n                    confirm-button-text='好的'\r\n                    cancel-button-text='不用了'\r\n                    icon=\"el-icon-info\"\r\n                    icon-color=\"red\"\r\n                    title=\"是否批量确认删除?\"\r\n                    @confirm=\"deleteByIds\"\r\n            >\r\n                <el-button type=\"danger\" slot=\"reference\" class=\"ml-5\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n            </el-popconfirm>\r\n        </div>\r\n\r\n        <el-table\r\n                :data=\"tableData\" border stripe\r\n                :header-cell-class-name=\"headerBg\"\r\n                @selection-change=\"handleSelectionChange\">\r\n            <!--            多选框-->\r\n            <el-table-column\r\n                    type=\"selection\"\r\n                    align=\"center\"\r\n                    width=\"50\">\r\n            </el-table-column>\r\n            <el-table-column  prop=\"id\" align=\"center\" label=\"ID\" width=\"40\"></el-table-column>\r\n            <el-table-column prop=\"name\" align=\"center\" label=\"文件名称\"></el-table-column>\r\n            <el-table-column prop=\"type\" align=\"center\" label=\"文件类型\"></el-table-column>\r\n            <el-table-column prop=\"size\" align=\"center\" label=\"文件大小(kb)\"></el-table-column>\r\n            <el-table-column align=\"center\" label=\"下载\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button type=\"primary\" @click=\"download(scope.row.url)\">下载</el-button>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"enable\" align=\"center\" label=\"启用\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-switch v-model=\"scope.row.enable\"\r\n                               active-color=\"#13ce66\"\r\n                               inactive-color=\"#ccc\"\r\n                               @change=\"changeEnable(scope.row)\"\r\n\r\n                    ></el-switch>\r\n                </template>\r\n            </el-table-column>\r\n            <el-table-column fixed=\"right\"\r\n                             label=\"操作\"\r\n                             align=\"center\"\r\n                             width=\"150\">\r\n                <template slot-scope=\"scope\">\r\n\r\n                    <el-popconfirm\r\n                            confirm-button-text='好的'\r\n                            cancel-button-text='不用了'\r\n                            icon=\"el-icon-info\"\r\n                            icon-color=\"red\"\r\n                            title=\"是否确认删除?\"\r\n                            @confirm=\"handleDelete(scope.row.id)\"\r\n                    >\r\n                        <el-button style=\"margin-left: 2px;\"\r\n                                   size=\"mini\"\r\n                                   type=\"danger\"\r\n                                   slot=\"reference\">删除</el-button>\r\n                    </el-popconfirm>\r\n                </template>\r\n\r\n            </el-table-column>\r\n        </el-table>\r\n        <div style=\"padding: 10px 0\">\r\n            <!--\r\n            current-page：当前页码\r\n            page-sizes：分页条件数\r\n            page-size：分页大小-->\r\n            <el-pagination\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    :current-page=\"pageNum\"\r\n                    :page-sizes=\"[2, 5, 10, 20]\"\r\n                    :page-size=\"pageSize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"File\",\r\n        data(){\r\n            return{\r\n                tableData: [],\r\n                multipleSelection: [],\r\n                pageNum: 1,\r\n                pageSize: 5,\r\n                total: 0,\r\n                name: '',\r\n                isDelete: false,\r\n                headerBg:'headerBg'\r\n            }\r\n        },\r\n        mounted() {\r\n            this.load();\r\n        },\r\n\r\n        methods:{\r\n\r\n            load(){\r\n                this.request.get(\"/file/page\",{\r\n                    params: {\r\n                        pageNum:this.pageNum,\r\n                        pageSize:this.pageSize,\r\n                        name:this.name,\r\n                        isDelete:this.isDelete\r\n                    }\r\n                }).then(req => {\r\n                        this.tableData = req.data.records;\r\n                        this.total = req.data.total;\r\n\r\n                })\r\n\r\n            },\r\n            //复选框选中后执行的方法\r\n            handleSelectionChange(val) {\r\n                this.multipleSelection = val;\r\n                console.log(this.multipleSelection);\r\n\r\n            },\r\n            //分页\r\n            handleSizeChange(pageSize) {\r\n                this.pageSize = pageSize;\r\n                if (this.username ==null && this.email == null && this.address == null){\r\n                    this.load();\r\n                }else {\r\n                    this.selectByCondition();\r\n                }\r\n\r\n            },\r\n            handleCurrentChange(pageNum) {\r\n                this.pageNum = pageNum;\r\n                this.selectByCondition();\r\n            },\r\n\r\n            //表格数据操作\r\n            handleEdit(row) {\r\n                // this.form = row;\r\n                this.form = Object.assign({},row); //将row拷贝到空对象中，解决没点确定 表中数据改变的问题\r\n                this.dialogVisible = true;\r\n                this.formTitle = '修改用户信息';\r\n            },\r\n            handleFileUploadSuccess(){\r\n                this.load();\r\n            },\r\n            //重置\r\n            reset(){\r\n                this.name = '';\r\n                this.load();\r\n            },\r\n            //文件下载\r\n            download(url){\r\n                window.open(url)\r\n            },\r\n\r\n            //设置 状态\r\n            changeEnable(row){\r\n                this.request.post(\"/file/enable\",row).then(res => {\r\n                    if (res.code === '200'){\r\n                        this.$message.success(\"操作成功\");\r\n\r\n                    }else{\r\n                        this.$message.error(\"操作失败\");\r\n                    }\r\n                })\r\n            },\r\n\r\n            //数据删除\r\n            handleDelete(id) {\r\n                this.request(\"/file/delete/\"+id).then(resp => {\r\n                    if (resp.code === '200'){\r\n                        //    删除成功\r\n                        this.$message.success(\"删除成功\");\r\n                        this.load();\r\n                    }else {\r\n                        this.$message.error(\"删除失败\");\r\n                    }\r\n                })\r\n            },\r\n\r\n            //批量删除\r\n            deleteByIds(){\r\n                // 将对象类型 直接转为 id数组\r\n                // [{},{},{}] => {1,2,3}\r\n                let ids =  this.multipleSelection.map(v => v.id);\r\n                this.request.post(\"/file/deletes\",ids).then(resp => {\r\n                    if (resp){\r\n                        //    删除成功\r\n                        this.load();\r\n                        this.$message({\r\n                            message: '批量删除成功!',\r\n                            type: 'success'\r\n                        });\r\n                    }else {\r\n                        this.$message.error(\"删除失败\");\r\n                    }\r\n                })\r\n            },\r\n\r\n        }\r\n    }\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]},"metadata":{},"sourceType":"module"}
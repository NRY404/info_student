{"ast":null,"code":"import \"core-js/modules/es.array.map.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n// import request from \"../utils/request\";\nimport Aside from \"../components/Aside\";\nexport default {\n  name: 'HomeManage',\n  data: function data() {\n    return {\n      // 多选框数据\n      multipleSelection: [],\n      tableData: [],\n      //表单数据\n      total: 0,\n      //总条数\n      pageNum: 1,\n      pageSize: 2,\n      collapseBtnClass: 'el-icon-s-fold',\n      //收缩图标\n      isCollapse: false,\n      sideWidth: 200,\n      logoTextShow: true,\n      dialogVisible: false,\n      form: {},\n      formTitle: '',\n      username: '',\n      password: '',\n      nickname: '',\n      phone: '',\n      email: '',\n      address: '',\n      headerBg: 'headerBg'\n    };\n  },\n  mounted: function mounted() {\n    this.load();\n  },\n  //\n  components: {\n    Aside: Aside\n  },\n  methods: {\n    tableRowClassName: function tableRowClassName(_ref) {\n      var row = _ref.row,\n          rowIndex = _ref.rowIndex;\n\n      if (rowIndex === 1) {\n        return 'warning-row';\n      } else if (rowIndex === 3) {\n        return 'success-row';\n      }\n\n      return '';\n    },\n    //复选框选中后执行的方法\n    handleSelectionChange: function handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //点击收缩按钮，触发\n    collapse: function collapse() {\n      this.isCollapse = !this.isCollapse;\n\n      if (this.isCollapse) {\n        //收缩\n        this.sideWidth = 64;\n        this.logoTextShow = false;\n        this.collapseBtnClass = 'el-icon-s-unfold';\n      } else {\n        //展开\n        this.sideWidth = 200;\n        this.logoTextShow = true;\n        this.collapseBtnClass = 'el-icon-s-fold';\n      }\n    },\n    //分页\n    handleSizeChange: function handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n\n      if (this.username == null && this.email == null && this.address == null) {\n        this.load();\n      } else {\n        this.selectByCondition();\n      }\n    },\n    handleCurrentChange: function handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.selectByCondition();\n    },\n    //表格数据操作\n    handleEdit: function handleEdit(row) {\n      // this.form = row;\n      this.form = Object.assign({}, row); //将row拷贝到空对象中，解决没点确定 表中数据改变的问题\n\n      this.dialogVisible = true;\n      this.formTitle = '修改用户信息';\n    },\n    //数据删除\n    handleDelete: function handleDelete(id) {\n      var _this = this;\n\n      this.request(\"/user/delete/\" + id).then(function (resp) {\n        if (resp) {\n          //    删除成功\n          _this.load();\n\n          _this.$message({\n            message: '删除成功!',\n            type: 'success'\n          });\n        } else {\n          _this.$message.error(\"删除失败\");\n        }\n      });\n    },\n    //批量删除\n    deleteByIds: function deleteByIds() {\n      var _this2 = this;\n\n      // 将对象类型 直接转为 id数组\n      // [{},{},{}] => {1,2,3}\n      var ids = this.multipleSelection.map(function (v) {\n        return v.id;\n      });\n      this.request.post(\"/user/deletes\", ids).then(function (resp) {\n        if (resp) {\n          //    删除成功\n          _this2.load();\n\n          _this2.$message({\n            message: '批量删除成功!',\n            type: 'success'\n          });\n        } else {\n          _this2.$message.error(\"删除失败\");\n        }\n      });\n    },\n    load: function load() {\n      var _this3 = this;\n\n      this.request.get(\"/user/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize).then(function (req) {\n        _this3.tableData = req.data;\n        _this3.total = req.total;\n      }); // //  请求分页查询数据\n      // fetch(\"http://localhost:8081/user/page?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize).then(res => res.json()).then(res =>{\n      //     console.log(res);\n      //     this.tableData = res.data;\n      //     this.total = res.total;\n      // })\n    },\n    selectByCondition: function selectByCondition() {\n      var _this4 = this;\n\n      this.request.get(\"/user/page3?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize + \"&username=\" + this.username + \"&email=\" + this.email + \"&address=\" + this.address).then(function (req) {\n        _this4.tableData = req.data;\n        _this4.total = req.total;\n      }); //  请求分页查询数据\n      // fetch(\"http://localhost:8081/user/page3?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize+\"&username=\"+this.username+\"&email=\"+this.email+\"&address=\"+this.address).then(res => res.json()).then(res =>{\n      //     console.log(res);\n      //     this.tableData = res.data;\n      //     // this.tableData = res.records;\n      //     this.total = res.total;\n      // })\n    },\n    //重置\n    reset: function reset() {\n      this.username = '';\n      this.email = '';\n      this.address = '';\n      this.load();\n    },\n    //新增按钮\n    handleAdd: function handleAdd() {\n      this.dialogVisible = true;\n      this.formTitle = '添加用户';\n      this.form = {};\n    },\n    //新增数据表单创建方法 保存\n    addUser: function addUser() {\n      var _this5 = this;\n\n      this.request.post(\"/user/add\", this.form).then(function (resp) {\n        if (resp) {\n          //    添加成功\n          //    关闭窗口\n          _this5.dialogVisible = false;\n\n          _this5.load();\n\n          _this5.$message({\n            message: '操作成功!',\n            type: 'success'\n          });\n        } else {\n          _this5.$message.error(\"保存失败\");\n        }\n      }); // console.log(this.tableData);\n      // this.$axios({\n      //     method: \"post\",\n      //     url: \"http://localhost:8081/user/add\",\n      //     data: this.tableData\n      // }).then(function (resp) {\n      //     if (resp.data === \"success\"){\n      //         //    添加成功\n      //         //    关闭窗口\n      //         this.dialogVisible = false;\n      //\n      //         //    重新查询数据\n      //         this.load();\n      //         //    弹出消息提示\n      //         this.$message({\n      //             message: '添加成功!',\n      //             type: 'success'\n      //         })\n      //\n      //     }\n      // })\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAkKA;AACA;AACA;AACAA,oBADA;AAEAC,MAFA,kBAEA;AACA;AACA;AACAC,2BAFA;AAGAC,mBAHA;AAGA;AACAC,cAJA;AAIA;AACAC,gBALA;AAMAC,iBANA;AAOAC,wCAPA;AAOA;AACAC,uBARA;AASAC,oBATA;AAUAC,wBAVA;AAWAC,0BAXA;AAYAC,cAZA;AAeAC,mBAfA;AAgBAC,kBAhBA;AAiBAC,kBAjBA;AAkBAC,kBAlBA;AAmBAC,eAnBA;AAoBAC,eApBA;AAqBAC,iBArBA;AAsBAC;AAtBA;AAwBA,GA3BA;AA4BAC,SA5BA,qBA4BA;AACA;AACA,GA9BA;AAgCA;AACAC;AACAC;AADA,GAjCA;AAqCAC;AACAC,qBADA,mCACA;AAAA;AAAA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;;AACA;AACA,KARA;AAUA;AACAC,yBAXA,iCAWAC,GAXA,EAWA;AACA;AACAC;AAEA,KAfA;AAiBA;AACAC,YAlBA,sBAkBA;AACA;;AACA;AAAA;AACA;AACA;AACA;AAEA,OALA,MAKA;AAAA;AACA;AACA;AACA;AACA;AACA,KA9BA;AA+BA;AACAC,oBAhCA,4BAgCAxB,QAhCA,EAgCA;AACA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;AAEA,KAxCA;AAyCAyB,uBAzCA,+BAyCA1B,OAzCA,EAyCA;AACA;AACA;AACA,KA5CA;AA8CA;AACA2B,cA/CA,sBA+CAC,GA/CA,EA+CA;AACA;AACA,yCAFA,CAEA;;AACA;AACA;AACA,KApDA;AAsDA;AACAC,gBAvDA,wBAuDAC,EAvDA,EAuDA;AAAA;;AACA;AACA;AACA;AACA;;AACA;AACAC,4BADA;AAEAC;AAFA;AAIA,SAPA,MAOA;AACA;AACA;AACA,OAXA;AAYA,KApEA;AAsEA;AACAC,eAvEA,yBAuEA;AAAA;;AACA;AACA;AACA;AAAA;AAAA;AACA;AACA;AACA;AACA;;AACA;AACAF,8BADA;AAEAC;AAFA;AAIA,SAPA,MAOA;AACA;AACA;AACA,OAXA;AAYA,KAvFA;AA0FAE,QA1FA,kBA0FA;AAAA;;AACA;AAEA;AACA;AAEA,OALA,EADA,CASA;AACA;AACA;AACA;AACA;AACA;AAEA,KA1GA;AA4GAC,qBA5GA,+BA4GA;AAAA;;AAEA;AACA;AACA;AACA,OAHA,EAFA,CAMA;AACA;AACA;AACA;AACA;AACA;AACA;AAEA,KA1HA;AA4HA;AACAC,SA7HA,mBA6HA;AACA;AACA;AACA;AACA;AACA,KAlIA;AAoIA;AACAC,aArIA,uBAqIA;AACA;AACA;AACA;AACA,KAzIA;AA0IA;AACAC,WA3IA,qBA2IA;AAAA;;AAEA;AACA;AACA;AACA;AACA;;AACA;;AACA;AACAP,4BADA;AAEAC;AAFA;AAIA,SATA,MASA;AACA;AACA;AAGA,OAfA,EAFA,CAmBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AAEA;AApLA;AArCA","names":["name","data","multipleSelection","tableData","total","pageNum","pageSize","collapseBtnClass","isCollapse","sideWidth","logoTextShow","dialogVisible","form","formTitle","username","password","nickname","phone","email","address","headerBg","mounted","components","Aside","methods","tableRowClassName","handleSelectionChange","val","console","collapse","handleSizeChange","handleCurrentChange","handleEdit","row","handleDelete","id","message","type","deleteByIds","load","selectByCondition","reset","handleAdd","addUser"],"sourceRoot":"src/views","sources":["Manage.vue"],"sourcesContent":["<template>\n    <div class=\"home\" style=\"height: 100%;\">\n        <el-container style=\"height: 100%;\">\n            <!--      左侧导航栏-->\n            <el-aside class=\"el-aside\" :width=\"sideWidth+'px'\" style=\" height: 100%;\">\n                <Aside :is-collapse=\"isCollapse\"></Aside>\n            </el-aside>\n\n            <!--      右侧内容-->\n            <el-container>\n                <!--        头部-->\n                <el-header class=\"el-header\" style=\" font-size: 12px;display: flex;\">\n                    <div style=\"flex: 1;font-size: 20px;\">\n                        <span :class=\"collapseBtnClass\" style=\"cursor: pointer\" @click=\"collapse\"></span>\n                    </div>\n                    <div style=\"text-align: right;font-size: 12px;width: 200px;\"></div>\n                    <el-dropdown style=\"width: 80px;cursor: pointer\">\n                        <i class=\"el-icon-setting\" style=\"margin-right: 15px\"><span>王小虎</span></i>\n                        <el-dropdown-menu style=\"margin-right: 30px\" slot=\"dropdown\" >\n                            <el-dropdown-item>个人信息</el-dropdown-item>\n                            <el-dropdown-item>退出</el-dropdown-item>\n                        </el-dropdown-menu>\n                    </el-dropdown>\n                </el-header>\n                <!--        面包屑-->\n                <el-breadcrumb separator=\"/\" style=\"margin-top: 10px\">\n                    <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n                    <el-breadcrumb-item><a href=\"/\">活动管理</a></el-breadcrumb-item>\n                </el-breadcrumb>\n                <!--        右侧列表-->\n                <el-main>\n                    <div style=\"margin: 10px 0\">\n                        <el-input class=\"ml-5\" v-model=\"username\" style=\"width: 200px;\" suffix-icon=\"el-icon-edit\" placeholder=\"请输入姓名\"></el-input>\n                        <el-input class=\"ml-5\" v-model=\"email\" style=\"width: 200px;\" suffix-icon=\"el-icon-message\" placeholder=\"请输入邮箱\"></el-input>\n                        <el-input class=\"ml-5\" v-model=\"address\" style=\"width: 200px;\" suffix-icon=\"el-icon-location-information\" placeholder=\"请输入地址\"></el-input>\n                        <el-button style=\"margin-left: 5px;\" type=\"primary\" icon=\"el-icon-search\" @click=\"selectByCondition\">搜索</el-button>\n                        <el-button style=\"margin-left: 5px;\" type=\"warning\"  @click=\"reset\">重置</el-button>\n                    </div>\n\n                    <!--            新增-->\n                    <div style=\"margin: 10px 0\">\n                        <!--    添加数据的表单-->\n                        <el-dialog\n                                :title=\"formTitle\"\n                                :visible.sync=\"dialogVisible\"\n                                width=\"30%\">\n                            <el-form ref=\"form\" :model=\"form\" label-width=\"80px\" size=\"small\">\n                                <el-form-item label=\"姓名\">\n                                    <el-input v-model=\"form.username\"></el-input>\n                                </el-form-item>\n                                <el-form-item label=\"密码\">\n                                    <el-input v-model=\"form.password\"></el-input>\n                                </el-form-item>\n                                <el-form-item label=\"昵称\">\n                                    <el-input v-model=\"form.nickname\"></el-input>\n                                </el-form-item>\n                                <el-form-item label=\"电话\">\n                                    <el-input v-model=\"form.phone\"></el-input>\n                                </el-form-item>\n                                <el-form-item label=\"邮箱\">\n                                    <el-input v-model=\"form.email\"></el-input>\n                                </el-form-item>\n                                <el-form-item label=\"地址\">\n                                    <el-input type=\"textarea\" v-model=\"form.address\"></el-input>\n                                </el-form-item>\n                                <!--                <el-form-item label=\"状态\">-->\n                                <!--                  <el-switch v-model=\"\"-->\n                                <!--                             active-value=\"1\"-->\n                                <!--                             inactive-value=\"0\">-->\n                                <!--                  </el-switch>-->\n                                <!--                </el-form-item>-->\n\n                                <el-form-item>\n                                    <el-button type=\"primary\" @click=\"addUser\">提交</el-button>\n                                    <el-button @click=\"dialogVisible = false\" >取消</el-button>\n                                </el-form-item>\n                            </el-form>\n                        </el-dialog>\n                    </div>\n                    <div style=\"margin: 10px 0\">\n                        <el-button type=\"primary\" @click=\"handleAdd\" plain>新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\n                        <el-popconfirm\n                                confirm-button-text='好的'\n                                cancel-button-text='不用了'\n                                icon=\"el-icon-info\"\n                                icon-color=\"red\"\n                                title=\"是否批量确认删除?\"\n                                @confirm=\"deleteByIds\"\n                        >\n                            <el-button type=\"danger\" slot=\"reference\" class=\"ml-5\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n                        </el-popconfirm>\n                        <el-button type=\"info\" class=\"ml-5\" round>导入 <i class=\"el-icon-download\"></i></el-button>\n                        <el-button type=\"info\" round>导出 <i class=\"el-icon-upload2\"></i></el-button>\n                    </div>\n\n                    <el-table\n                            :data=\"tableData\" border stripe\n                            :header-cell-class-name=\"headerBg\"\n                            :row-class-name=\"tableRowClassName\"\n                            @selection-change=\"handleSelectionChange\">\n                        <!--            多选框-->\n                        <el-table-column\n                                type=\"selection\"\n                                align=\"center\"\n                                width=\"50\">\n                        </el-table-column>\n                        <el-table-column  prop=\"id\" align=\"center\" label=\"ID\" width=\"40\"></el-table-column>\n                        <el-table-column prop=\"username\" align=\"center\" label=\"姓名\" width=\"120\"></el-table-column>\n                        <el-table-column prop=\"nickname\" align=\"center\" label=\"昵称\"></el-table-column>\n                        <el-table-column prop=\"phone\" align=\"center\" label=\"电话\"></el-table-column>\n                        <el-table-column prop=\"email\" align=\"center\" label=\"邮箱\"></el-table-column>\n                        <el-table-column prop=\"address\" align=\"center\" label=\"地址\"></el-table-column>\n                        <el-table-column fixed=\"right\"\n                                         label=\"操作\"\n                                         align=\"center\"\n                                         width=\"150\">\n                            <template slot-scope=\"scope\">\n                                <el-button\n                                        size=\"mini\"\n                                        @click=\"handleEdit(scope.row)\">编辑</el-button>\n                                <el-popconfirm\n                                        confirm-button-text='好的'\n                                        cancel-button-text='不用了'\n                                        icon=\"el-icon-info\"\n                                        icon-color=\"red\"\n                                        title=\"是否确认删除?\"\n                                        @confirm=\"handleDelete(scope.row.id)\"\n                                >\n                                    <el-button style=\"margin-left: 2px;\"\n                                            size=\"mini\"\n                                            type=\"danger\"\n                                            slot=\"reference\">删除</el-button>\n                                </el-popconfirm>\n                            </template>\n\n                        </el-table-column>\n                    </el-table>\n                    <div style=\"padding: 10px 0\">\n                        <!--\n                        current-page：当前页码\n                        page-sizes：分页条件数\n                        page-size：分页大小-->\n                        <el-pagination\n                                @size-change=\"handleSizeChange\"\n                                @current-change=\"handleCurrentChange\"\n                                :current-page=\"pageNum\"\n                                :page-sizes=\"[2, 5, 10, 20]\"\n                                :page-size=\"pageSize\"\n                                layout=\"total, sizes, prev, pager, next, jumper\"\n                                :total=\"total\">\n                        </el-pagination>\n                    </div>\n                </el-main>\n\n            </el-container>\n        </el-container>\n    </div>\n</template>\n\n\n<script>\n\n    // import request from \"../utils/request\";\n    import Aside from \"../components/Aside\";\n    export default {\n        name: 'HomeManage',\n        data(){\n            return{\n                // 多选框数据\n                multipleSelection:[],\n                tableData: [],  //表单数据\n                total:0,  //总条数\n                pageNum: 1,\n                pageSize: 2,\n                collapseBtnClass:'el-icon-s-fold',//收缩图标\n                isCollapse:false,\n                sideWidth:200,\n                logoTextShow: true,\n                dialogVisible:false,\n                form: {\n\n                },\n                formTitle: '',\n                username:'',\n                password:'',\n                nickname: '',\n                phone: '',\n                email:'',\n                address:'',\n                headerBg:'headerBg'\n            }\n        },\n        mounted() {\n            this.load();\n        },\n\n        //\n        components:{\n            Aside\n        },\n\n        methods:{\n            tableRowClassName({row, rowIndex}) {\n                if (rowIndex === 1) {\n                    return 'warning-row';\n                } else if (rowIndex === 3) {\n                    return 'success-row';\n                }\n                return '';\n            },\n\n            //复选框选中后执行的方法\n            handleSelectionChange(val) {\n                this.multipleSelection = val;\n                console.log(this.multipleSelection);\n\n            },\n\n            //点击收缩按钮，触发\n            collapse(){\n                this.isCollapse = !this.isCollapse;\n                if (this.isCollapse){//收缩\n                    this.sideWidth = 64;\n                    this.logoTextShow = false;\n                    this.collapseBtnClass = 'el-icon-s-unfold'\n\n                }else{    //展开\n                    this.sideWidth = 200;\n                    this.logoTextShow = true;\n                    this.collapseBtnClass = 'el-icon-s-fold'\n                }\n            },\n            //分页\n            handleSizeChange(pageSize) {\n                this.pageSize = pageSize;\n                if (this.username ==null && this.email == null && this.address == null){\n                    this.load();\n                }else {\n                    this.selectByCondition();\n                }\n\n            },\n            handleCurrentChange(pageNum) {\n                this.pageNum = pageNum;\n                this.selectByCondition();\n            },\n\n            //表格数据操作\n            handleEdit(row) {\n                // this.form = row;\n                this.form = Object.assign({},row); //将row拷贝到空对象中，解决没点确定 表中数据改变的问题\n                this.dialogVisible = true;\n                this.formTitle = '修改用户信息';\n            },\n\n            //数据删除\n            handleDelete(id) {\n                this.request(\"/user/delete/\"+id).then(resp => {\n                    if (resp){\n                        //    删除成功\n                        this.load();\n                        this.$message({\n                            message: '删除成功!',\n                            type: 'success'\n                        });\n                    }else {\n                        this.$message.error(\"删除失败\");\n                    }\n                })\n            },\n\n            //批量删除\n            deleteByIds(){\n                // 将对象类型 直接转为 id数组\n                // [{},{},{}] => {1,2,3}\n                let ids =  this.multipleSelection.map(v => v.id);\n                this.request.post(\"/user/deletes\",ids).then(resp => {\n                    if (resp){\n                        //    删除成功\n                        this.load();\n                        this.$message({\n                            message: '批量删除成功!',\n                            type: 'success'\n                        });\n                    }else {\n                        this.$message.error(\"删除失败\");\n                    }\n                })\n            },\n\n\n            load(){\n                this.request.get(\"/user/page?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize).then(req => {\n\n                    this.tableData = req.data;\n                    this.total = req.total;\n\n                })\n\n\n                // //  请求分页查询数据\n                // fetch(\"http://localhost:8081/user/page?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize).then(res => res.json()).then(res =>{\n                //     console.log(res);\n                //     this.tableData = res.data;\n                //     this.total = res.total;\n                // })\n\n            },\n\n            selectByCondition(){\n\n                this.request.get(\"/user/page3?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize+\"&username=\"+this.username+\"&email=\"+this.email+\"&address=\"+this.address).then(req => {\n                    this.tableData = req.data;\n                    this.total = req.total;\n                })\n                //  请求分页查询数据\n                // fetch(\"http://localhost:8081/user/page3?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize+\"&username=\"+this.username+\"&email=\"+this.email+\"&address=\"+this.address).then(res => res.json()).then(res =>{\n                //     console.log(res);\n                //     this.tableData = res.data;\n                //     // this.tableData = res.records;\n                //     this.total = res.total;\n                // })\n\n            },\n\n            //重置\n            reset(){\n                this.username = '';\n                this.email = '';\n                this.address = '';\n                this.load();\n            },\n\n            //新增按钮\n            handleAdd(){\n                this.dialogVisible = true;\n                this.formTitle = '添加用户';\n                this.form = {};\n            },\n            //新增数据表单创建方法 保存\n            addUser(){\n\n                this.request.post(\"/user/add\",this.form).then(resp => {\n                    if (resp){\n                        //    添加成功\n                        //    关闭窗口\n                        this.dialogVisible = false;\n                        this.load();\n                        this.$message({\n                            message: '操作成功!',\n                            type: 'success'\n                        });\n                    }else {\n                        this.$message.error(\"保存失败\");\n                    }\n\n\n                });\n\n                // console.log(this.tableData);\n                // this.$axios({\n                //     method: \"post\",\n                //     url: \"http://localhost:8081/user/add\",\n                //     data: this.tableData\n                // }).then(function (resp) {\n                //     if (resp.data === \"success\"){\n                //         //    添加成功\n                //         //    关闭窗口\n                //         this.dialogVisible = false;\n                //\n                //         //    重新查询数据\n                //         this.load();\n                //         //    弹出消息提示\n                //         this.$message({\n                //             message: '添加成功!',\n                //             type: 'success'\n                //         })\n                //\n                //     }\n                // })\n\n            }\n\n        }\n\n    }\n</script>\n<style>\n    .el-header {\n        background-color: #B3C0D1;\n        color: #333;\n        line-height: 60px;\n    }\n\n    .headerBg{\n        background-color: #d7dde5 !important;\n    }\n\n</style>"]},"metadata":{},"sourceType":"module"}
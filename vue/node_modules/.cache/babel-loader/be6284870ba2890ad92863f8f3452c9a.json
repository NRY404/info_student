      message: '添加成功!',\n            type: 'success'\n          });\n        } else {\n          _this2.$message.error(\"保存失败\");\n        }\n      });\n    },\n    handleSizeChange: function handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n\n      if (this.className == null || this.className === '') {\n        this.load();\n      } else {\n        this.selectByCondition();\n      }\n    },\n    handleCurrentChange: function handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n\n      if (this.className == null || this.className === '') {\n        this.load();\n      } else {\n        this.selectByCondition();\n      }\n    },\n    //表格数据操作\n    handleEdit: function handleEdit(row) {\n      var _this3 = this;\n\n      this.request.get(\"/class/getStu?pageNumS=\" + this.pageNumS + \"&pageSizeS=\" + this.pageSizeS + \"&classId=\" + row.classId).then(function (req) {\n        if (req.data == null) {\n          _this3.$message({\n            message: '该班级未加入学生！',\n            type: 'warning'\n          });\n        } else {\n          _this3.studentData = req.data;\n          _this3.show = !_this3.show;\n          _this3.totalS = req.total;\n        }\n      });\n    },\n    handleSizeChangeStu: function handleSizeChangeStu(pageSize) {\n      this.pageSizeS = pageSize;\n      this.handleEdit(this.studentData[0]);\n      this.show = false;\n    },\n    handleCurrentChangeStu: function handleCurrentChangeStu(pageNum) {\n      this.pageNumS = pageNum;\n      this.handleEdit(this.studentData[0]);\n      this.show = false;\n    },\n    selectByCondition: function selectByCondition() {\n      var _this4 = this;\n\n      this.request.get(\"/class/page3?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize + \"&className=\" + this.className).then(function (req) {\n        _this4.tableData = req.data;\n        _this4.total = req.total;\n      });\n    },\n    //重置\n    reset: function reset() {\n      this.name = '';\n      this.load();\n    },\n    
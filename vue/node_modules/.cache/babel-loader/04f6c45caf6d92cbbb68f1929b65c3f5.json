{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: \"StudentView\",\n  props: {\n    isCollapse: Boolean,\n    logoTextShow: Boolean\n  },\n\n  data() {\n    return {\n      // 多选框数据\n      multipleSelection: [],\n      tableData: [],\n      //表单数据\n      total: 0,\n      //总条数\n      pageNum: 1,\n      pageSize: 10,\n      dialogVisible: false,\n      form: {},\n      dormL: {},\n      classL: {},\n      formTitle: '',\n      studentId: '',\n      name: '',\n      gender: '',\n      classId: '',\n      className: '',\n      dormName: '',\n      dormId: '',\n      phone: '',\n      email: '',\n      birthday: '',\n      address: '',\n      avatar: '',\n      headerBg: 'headerBg',\n      genderN: [{\n        gType: '0',\n        label: '男'\n      }, {\n        gType: '1',\n        label: '女'\n      }, {\n        gType: '2',\n        label: '零'\n      }],\n      dorm: [{\n        dormId: '101',\n        label: '公寓01-101'\n      }],\n      classN: [{\n        classId: '2019101',\n        label: '磨角193'\n      }]\n    };\n  },\n\n  mounted() {\n    this.load();\n  },\n\n  methods: {\n    tableRowClassName({\n      row,\n      rowIndex\n    }) {\n      if (rowIndex === 1) {\n        return 'warning-row';\n      } else if (rowIndex === 3) {\n        return 'success-row';\n      }\n\n      return '';\n    },\n\n    load() {\n      this.request.get(\"/student/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize).then(req => {\n        this.tableData = req.data;\n        this.total = req.total;\n      });\n      this.request.get(\"/class/\").then(req => {\n        this.classN = req;\n        let i = 0;\n\n        for (i = 0; i < req.length; i++) {\n          this.classN[i].classId = req[i].classId;\n          this.classN[i].label = req[i].className;\n        }\n      });\n      this.request.get(\"/dorm/\").then(req => {\n        this.dorm = req;\n        let i = 0;\n\n        for (i = 0; i < req.length; i++) {\n          this.dorm[i].dormId = req[i].dormId;\n          this.dorm[i].label = req[i].dormName;\n        }\n      });\n    },\n\n    //复选框选中后执行的方法\n    handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n\n    //分页\n    handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n\n      if (this.studentId == null && this.name == null && this.email == null) {\n        this.load();\n      } else {\n        this.selectByCondition();\n      }\n    },\n\n    handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.selectByCondition();\n    },\n\n    //表格数据操作\n    handleEdit(row) {\n      // this.form = row;\n      this.form = Object.assign({}, row); //将row拷贝到空对象中，解决没点确定 表中数据改变的问题\n\n      this.dialogVisible = true;\n      this.formTitle = '修改学生信息';\n    },\n\n    //数据删除\n    handleDelete(studentId) {\n      this.$confirm(\"此操作永久删除当前数据\", \"删除\", {\n        type: 'info'\n      }).then(() => {\n        this.request(\"/student/delete/\" + studentId).then(resp => {\n          if (resp) {\n            // //    删除成功\n            // this.load();\n            this.$message({\n              message: '删除成功!',\n              type: 'success'\n            });\n          } else {\n            this.$message.error(\"删除失败\");\n          }\n        }).finally(() => {\n          this.load();\n        });\n      }).catch(() => {\n        this.$message.info(\"取消删除\");\n      });\n    },\n\n    //批量删除\n    deleteByIds() {\n      // 将对象类型 直接转为 id数组\n      // [{},{},{}] => {1,2,3}\n      let studentIds = this.multipleSelection.map(v => v.studentId);\n      this.request.post(\"/student/deletes\", studentIds).then(resp => {\n        if (resp) {\n          //    删除成功\n          this.load();\n          this.$message({\n            message: '批量删除成功!',\n            type: 'success'\n          });\n        } else {\n          this.$message.error(\"删除失败\");\n        }\n      });\n    },\n\n    selectByCondition() {\n      this.request.get(\"/student/page3?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize + \"&studentId=\" + this.studentId + \"&name=\" + this.name + \"&email=\" + this.email).then(req => {\n        this.tableData = req.data;\n        this.total = req.total;\n      }); //  请求分页查询数据\n      // fetch(\"http://nxyi.ink:9090/user/page3?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize+\"&username=\"+this.username+\"&email=\"+this.email+\"&address=\"+this.address).then(res => res.json()).then(res =>{\n      //     console.log(res);\n      //     this.tableData = res.data;\n      //     // this.tableData = res.records;\n      //     this.total = res.total;\n      // })\n    },\n\n    //重置\n    reset() {\n      this.studentId = '';\n      this.name = '';\n      this.email = '';\n      this.load();\n    },\n\n    //新增按钮\n    handleAdd() {\n      this.dialogVisible = true;\n      this.formTitle = '添加学生';\n      this.form = {};\n    },\n\n    //新增数据表单创建方法 保存\n    addStudent() {\n      this.request.post(\"/student/add\", this.form).then(resp => {\n        if (resp) {\n          //判断宿舍是否已满员\n          if (resp.code === '123') {\n            this.$message({\n              message: resp.msg,\n              type: 'warning'\n            });\n            return;\n          } //    添加成功\n          //    关闭窗口\n\n\n          this.dialogVisible = false;\n          this.form.avatar = resp.avatar;\n          this.load();\n          this.$message({\n            message: '操作成功!',\n            type: 'success'\n          });\n        } else {\n          this.$message.error(\"保存失败\");\n        }\n      });\n    },\n\n    //变换班级号\n    changeCName(classId) {\n      this.form.classL = this.classN.find(item => item.classId === classId);\n      this.form.classId = this.form.classL.classId;\n      this.form.className = this.form.classL.label;\n    },\n\n    //变换宿舍号\n    changeDName(dormId) {\n      this.form.dormL = this.dorm.find(item => item.dormId === dormId);\n      this.form.dormId = this.form.dormL.dormId;\n      this.form.dormName = this.form.dormL.label;\n    },\n\n    //          头像上传成功\n    handleAvatarSuccess(res) {\n      this.form.avatar = res;\n    },\n\n    beforeAvatarUpload(file) {\n      const isJPG = file.type === 'image/jpeg' || 'image/png';\n      const isLt2M = file.size / 1024 / 1024 < 2;\n\n      if (!isJPG) {\n        this.$message.error('上传头像图片只能是 JPG 格式!');\n      }\n\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!');\n      }\n\n      return isJPG && isLt2M;\n    },\n\n    //    导入\n    handleImportSuccess() {\n      this.$message.success(\"文件上传成功\");\n      this.load();\n    },\n\n    //    导出\n    exportStudents() {\n      window.open(\"http://nxyi.ink:9090/student/export\");\n    }\n\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmLA;AACAA,qBADA;AAEAC;AACAC,uBADA;AAEAC;AAFA,GAFA;;AAMAC;AACA;AACA;AACAC,2BAFA;AAGAC,mBAHA;AAGA;AACAC,cAJA;AAIA;AACAC,gBALA;AAMAC,kBANA;AAOAC,0BAPA;AAQAC,cARA;AASAC,eATA;AAUAC,gBAVA;AAWAC,mBAXA;AAYAC,mBAZA;AAaAf,cAbA;AAcAgB,gBAdA;AAeAC,iBAfA;AAgBAC,mBAhBA;AAiBAC,kBAjBA;AAkBAC,gBAlBA;AAmBAC,eAnBA;AAoBAC,eApBA;AAqBAC,kBArBA;AAsBAC,iBAtBA;AAuBAC,gBAvBA;AAwBAC,0BAxBA;AAyBAC,gBACA;AACAC,kBADA;AAEAC;AAFA,OADA,EAKA;AACAD,kBADA;AAEAC;AAFA,OALA,EASA;AACAD,kBADA;AAEAC;AAFA,OATA,CAzBA;AAuCAC;AACAV,qBADA;AAEAS;AAFA,QAvCA;AA2CAE;AACAd,0BADA;AAEAY;AAFA;AA3CA;AAgDA,GAvDA;;AAyDAG;AACA;AACA,GA3DA;;AA6DAC;AAEAC;AAAAC;AAAAC;AAAA;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;;AACA;AACA,KATA;;AAUAC;AACA,+FACAC,IADA,CACAC;AACA;AACA;AACA,OAJA;AAKA;AACA;AACA;;AACA;AACA;AACA;AACA;AACA,OAPA;AAQA;AACA;AACA;;AACA;AACA;AACA;AACA;AACA,OAPA;AAQA,KAhCA;;AAkCA;AACAC;AACA;AACAC;AACA,KAtCA;;AAsCA;AACAC;AACA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;AAEA,KA/CA;;AAgDAC;AACA;AACA;AACA,KAnDA;;AAqDA;AACAC;AACA;AACA,yCAFA,CAEA;;AACA;AACA;AACA,KA3DA;;AA6DA;AACAC;AACA;AACAC;AADA,SAEAR,IAFA,CAEA;AACA;AACA;AACA;AACA;AACA;AACAS,8BADA;AAEAD;AAFA;AAIA,WAPA,MAOA;AACA;AACA;AACA,SAXA,EAWAE,OAXA,CAWA;AACA;AACA,SAbA;AAcA,OAjBA,EAiBAC,KAjBA,CAiBA;AACA;AACA,OAnBA;AAqBA,KApFA;;AAsFA;AACAC;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACAH,8BADA;AAEAD;AAFA;AAIA,SAPA,MAOA;AACA;AACA;AACA,OAXA;AAYA,KAvGA;;AAyGAK;AAEA;AACA;AACA;AACA,OAHA,EAFA,CAMA;AACA;AACA;AACA;AACA;AACA;AACA;AAEA,KAvHA;;AAyHA;AACAC;AACA;AACA;AACA;AACA;AACA,KA/HA;;AAiIA;AACAC;AACA;AACA;AACA;AACA,KAtIA;;AAuIA;AACAC;AAEA;AACA;AACA;AACA;AACA;AACAP,+BADA;AAEAD;AAFA;AAIA;AACA,WARA,CAUA;AACA;;;AACA;AACA;AACA;AACA;AACAC,4BADA;AAEAD;AAFA;AAIA,SAnBA,MAmBA;AACA;AACA;AACA,OAvBA;AAwBA,KAlKA;;AAoKA;AACAS;AACA;AACA;AACA;AACA,KAzKA;;AA0KA;AACAC;AACA;AACA;AACA;AACA,KA/KA;;AAiLA;AACAC;AACA;AACA,KApLA;;AAqLAC;AACA;AACA;;AAEA;AACA;AACA;;AACA;AACA;AACA;;AACA;AACA,KAhMA;;AAkMA;AACAC;AACA;AACA;AACA,KAtMA;;AAwMA;AACAC;AACAC;AACA;;AA3MA;AA7DA","names":["name","props","isCollapse","logoTextShow","data","multipleSelection","tableData","total","pageNum","pageSize","dialogVisible","form","dormL","classL","formTitle","studentId","gender","classId","className","dormName","dormId","phone","email","birthday","address","avatar","headerBg","genderN","gType","label","dorm","classN","mounted","methods","tableRowClassName","row","rowIndex","load","then","req","handleSelectionChange","console","handleSizeChange","handleCurrentChange","handleEdit","handleDelete","type","message","finally","catch","deleteByIds","selectByCondition","reset","handleAdd","addStudent","changeCName","changeDName","handleAvatarSuccess","beforeAvatarUpload","handleImportSuccess","exportStudents","window"],"sourceRoot":"src/views","sources":["UserView.vue"],"sourcesContent":["<template>\r\n    <div>\r\n<!--        顶部搜索框 -->\r\n        <div style=\"margin: 10px 0\">\r\n            <el-input class=\"ml-5\" v-model=\"studentId\" style=\"width: 200px;\" suffix-icon=\"el-icon-edit\" placeholder=\"请输入学号\"></el-input>\r\n            <el-input class=\"ml-5\" v-model=\"name\" style=\"width: 200px;\" suffix-icon=\"el-icon-edit\" placeholder=\"请输入姓名\"></el-input>\r\n            <el-input class=\"ml-5\" v-model=\"email\" style=\"width: 200px;\" suffix-icon=\"el-icon-message\" placeholder=\"请输入邮箱\"></el-input>\r\n            <el-button style=\"margin-left: 5px;\" type=\"primary\" icon=\"el-icon-search\" @click=\"selectByCondition\">搜索</el-button>\r\n            <el-button style=\"margin-left: 5px;\" type=\"warning\"  @click=\"reset\">重置</el-button>\r\n        </div>\r\n\r\n        <!--            新增 dialog-->\r\n        <div style=\"margin: 10px 0\">\r\n            <!--    添加数据的表单-->\r\n            <el-dialog\r\n                    :title=\"formTitle\"\r\n                    :visible.sync=\"dialogVisible\"\r\n                    width=\"30%\">\r\n                <el-form ref=\"form\" :model=\"form\" label-width=\"80px\" size=\"small\">\r\n                    <el-form-item label=\"头像\" style=\"line-height: 50px;height: 50px\">\r\n                        <el-upload\r\n                                class=\"avatar-uploader\"\r\n                                action=\"http://nxyi.ink:9090/file/upload\"\r\n                                accept=\".jpg,.jpeg,.png,.JPG,.JPEG\"\r\n                                :show-file-list=\"false\"\r\n                                :on-success=\"handleAvatarSuccess\"\r\n                                :before-upload=\"beforeAvatarUpload\">\r\n                            <img style=\"width: 50px;border-radius: 50%;  left: 35px; cursor: pointer;\r\n                                        display: block;\r\n                                        box-shadow: 3px 5px 5px rgba(135,206,235, 0.3);\"\r\n                                 v-if=\"form.avatar\" :src=\"form.avatar\" alt=\"头像\" class=\"avatar\">\r\n                            <i v-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n                        </el-upload>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"学号\">\r\n                        <el-input v-model=\"form.studentId\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"姓名\">\r\n                        <el-input v-model=\"form.name\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"性别\">\r\n                        <el-select v-model=\"form.gender\" placeholder=\"请选择\" @change=\"changeCName\">\r\n                            <el-option\r\n                                    v-for=\"item in genderN\"\r\n                                    :key=\"item.gType\"\r\n                                    :label=\"item.label\"\r\n                                    :value=\"item.label\">\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n\r\n                    <el-form-item label=\"班级\">\r\n                        <el-select v-model=\"form.className\" placeholder=\"请选择\" @change=\"changeCName\">\r\n                            <el-option\r\n                                    v-for=\"item in classN\"\r\n                                    :key=\"item.classId\"\r\n                                    :label=\"item.label\"\r\n                                    :value=\"item.classId\">\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"宿舍\">\r\n                        <el-select v-model=\"form.dormName\" placeholder=\"请选择\" @change=\"changeDName\">\r\n                            <el-option\r\n                                    v-for=\"item in dorm\"\r\n                                    :key=\"item.dormId\"\r\n                                    :label=\"item.label\"\r\n                                    :value=\"item.dormId\">\r\n                            </el-option>\r\n                        </el-select>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"电话\">\r\n                        <el-input v-model=\"form.phone\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"邮箱\">\r\n                        <el-input v-model=\"form.email\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"出生日期\">\r\n                        <el-date-picker\r\n                                v-model=\"form.birthday\"\r\n                                type=\"datetime\"\r\n                                value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n                                placeholder=\"选择日期时间\">\r\n                        </el-date-picker>\r\n                    </el-form-item>\r\n                    <el-form-item label=\"地址\">\r\n                        <el-input type=\"textarea\" v-model=\"form.address\"></el-input>\r\n                    </el-form-item>\r\n                    <el-form-item>\r\n                        <el-button type=\"primary\" @click=\"addStudent\">提交</el-button>\r\n                        <el-button @click=\"dialogVisible = false\" >取消</el-button>\r\n                    </el-form-item>\r\n            </el-form>\r\n            </el-dialog>\r\n        </div>\r\n<!--        功能区-新增、删除、导出、导入-->\r\n        <div style=\"margin: 10px 0\">\r\n            <el-button type=\"primary\" @click=\"handleAdd\" plain>新增 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\r\n            <el-popconfirm\r\n                    confirm-button-text='好的'\r\n                    cancel-button-text='不用了'\r\n                    icon=\"el-icon-info\"\r\n                    icon-color=\"red\"\r\n                    title=\"是否批量确认删除?\"\r\n                    @confirm=\"deleteByIds\"\r\n            >\r\n                <el-button type=\"danger\" slot=\"reference\" class=\"ml-5\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\r\n            </el-popconfirm>\r\n            <el-upload action=\"http://nxyi.ink:9090/student/import\"\r\n                       :show-file-list=\"false\"\r\n                       accept=\".xlsx,.xls\"\r\n                       :on-success=\"handleImportSuccess\"\r\n                       style=\"display: inline-block;margin-left: 5px;\">\r\n                <el-button type=\"info\" round>导入<i class=\"el-icon-download\"></i></el-button>\r\n            </el-upload>\r\n            <el-button type=\"info\" @click=\"exportStudents\" class=\"ml-5\" round>导出 <i class=\"el-icon-upload2\"></i></el-button>\r\n        </div>\r\n<!--    数据区-->\r\n        <el-table\r\n                :data=\"tableData\" border stripe\r\n                :header-cell-class-name=\"headerBg\"\r\n                :row-class-name=\"tableRowClassName\"\r\n                @selection-change=\"handleSelectionChange\">\r\n            <!--            多选框-->\r\n            <el-table-column type=\"selection\" align=\"center\" width=\"50\"></el-table-column>\r\n            <el-table-column  prop=\"studentId\" align=\"center\" label=\"学号\" width=\"100\"></el-table-column>\r\n            <el-table-column prop=\"name\" align=\"center\" label=\"姓名\" width=\"120\"></el-table-column>\r\n            <el-table-column prop=\"gender\" align=\"center\" label=\"性别\" width=\"50\"></el-table-column>\r\n            <el-table-column prop=\"className\" align=\"center\" label=\"班级\"></el-table-column>\r\n            <el-table-column prop=\"dormName\" align=\"center\" label=\"宿舍\"></el-table-column>\r\n            <el-table-column prop=\"phone\" align=\"center\" label=\"电话\"></el-table-column>\r\n            <el-table-column prop=\"email\" align=\"center\" label=\"邮箱\"></el-table-column>\r\n            <el-table-column prop=\"birthday\" align=\"center\" label=\"出生日期\"></el-table-column>\r\n            <el-table-column prop=\"address\" align=\"center\" label=\"地址\"></el-table-column>\r\n            <el-table-column fixed=\"right\"\r\n                             label=\"操作\"\r\n                             align=\"center\"\r\n                             width=\"150\">\r\n                <template slot-scope=\"scope\">\r\n                    <el-button\r\n                            size=\"mini\"\r\n                            @click=\"handleEdit(scope.row)\">编辑</el-button>\r\n                    <el-popconfirm\r\n                            confirm-button-text='好的'\r\n                            cancel-button-text='不用了'\r\n                            icon=\"el-icon-info\"\r\n                            icon-color=\"red\"\r\n                            title=\"是否确认删除?\"\r\n                            @confirm=\"handleDelete(scope.row.studentId)\"\r\n                    >\r\n                        <el-button style=\"margin-left: 2px;\"\r\n                                   size=\"mini\"\r\n                                   type=\"danger\"\r\n                                   slot=\"reference\">删除</el-button>\r\n                    </el-popconfirm>\r\n                </template>\r\n\r\n            </el-table-column>\r\n        </el-table>\r\n<!--        分页-->\r\n        <div style=\"padding: 10px 0\">\r\n            <!--\r\n            current-page：当前页码\r\n            page-sizes：分页条件数\r\n            page-size：分页大小-->\r\n            <el-pagination\r\n                    @size-change=\"handleSizeChange\"\r\n                    @current-change=\"handleCurrentChange\"\r\n                    :current-page=\"pageNum\"\r\n                    :page-sizes=\"[2, 5, 10, 20]\"\r\n                    :page-size=\"pageSize\"\r\n                    layout=\"total, sizes, prev, pager, next, jumper\"\r\n                    :total=\"total\">\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    export default {\r\n        name: \"StudentView\",\r\n        props:{\r\n            isCollapse: Boolean,\r\n            logoTextShow: Boolean\r\n        },\r\n        data(){\r\n            return{\r\n                // 多选框数据\r\n                multipleSelection:[],\r\n                tableData: [],  //表单数据\r\n                total:0,  //总条数\r\n                pageNum: 1,\r\n                pageSize: 10,\r\n                dialogVisible:false,\r\n                form: { },\r\n                dormL: { },\r\n                classL: { },\r\n                formTitle: '',\r\n                studentId:'',\r\n                name:'',\r\n                gender:'',\r\n                classId: '',\r\n                className:'',\r\n                dormName:'',\r\n                dormId: '',\r\n                phone: '',\r\n                email:'',\r\n                birthday:'',\r\n                address:'',\r\n                avatar: '',\r\n                headerBg:'headerBg',\r\n                genderN: [\r\n                    {\r\n                      gType: '0',\r\n                      label: '男'\r\n                    },\r\n                    {\r\n                      gType: '1',\r\n                      label: '女'\r\n                    },\r\n                    {\r\n                      gType: '2',\r\n                      label: '零'\r\n                    }\r\n                    ],\r\n                dorm: [{\r\n                    dormId: '101',\r\n                    label: '公寓01-101'\r\n                }],\r\n                classN: [{\r\n                    classId: '2019101',\r\n                    label: '磨角193'\r\n                }],\r\n            }\r\n        },\r\n\r\n        mounted() {\r\n            this.load();\r\n        },\r\n\r\n        methods:{\r\n\r\n            tableRowClassName({row, rowIndex}) {\r\n                if (rowIndex === 1) {\r\n                    return 'warning-row';\r\n                } else if (rowIndex === 3) {\r\n                    return 'success-row';\r\n                }\r\n                return '';\r\n            },\r\n            load(){\r\n                this.request.get(\"/student/page?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize).\r\n                then(req => {\r\n                    this.tableData = req.data;\r\n                    this.total = req.total;\r\n                });\r\n                this.request.get(\"/class/\").then(req => {\r\n                    this.classN = req;\r\n                    let i = 0;\r\n                    for (i = 0; i < req.length; i++) {\r\n                        this.classN[i].classId = req[i].classId;\r\n                        this.classN[i].label = req[i].className;\r\n                    }\r\n                });\r\n                this.request.get(\"/dorm/\").then(req => {\r\n                    this.dorm = req;\r\n                    let i = 0;\r\n                    for (i = 0; i < req.length; i++) {\r\n                        this.dorm[i].dormId = req[i].dormId;\r\n                        this.dorm[i].label = req[i].dormName;\r\n                    }\r\n                });\r\n            },\r\n\r\n            //复选框选中后执行的方法\r\n            handleSelectionChange(val) {\r\n                this.multipleSelection = val;\r\n                console.log(this.multipleSelection);\r\n            },//分页\r\n            handleSizeChange(pageSize) {\r\n                this.pageSize = pageSize;\r\n                if (this.studentId ==null && this.name == null && this.email == null){\r\n                    this.load();\r\n                }else {\r\n                    this.selectByCondition();\r\n                }\r\n\r\n            },\r\n            handleCurrentChange(pageNum) {\r\n                this.pageNum = pageNum;\r\n                this.selectByCondition();\r\n            },\r\n\r\n            //表格数据操作\r\n            handleEdit(row) {\r\n                // this.form = row;\r\n                this.form = Object.assign({},row); //将row拷贝到空对象中，解决没点确定 表中数据改变的问题\r\n                this.dialogVisible = true;\r\n                this.formTitle = '修改学生信息';\r\n            },\r\n\r\n            //数据删除\r\n            handleDelete(studentId) {\r\n                this.$confirm(\"此操作永久删除当前数据\",\"删除\",{\r\n                    type:'info'\r\n                }).then(()=>{\r\n                    this.request(\"/student/delete/\"+studentId).then(resp => {\r\n                        if (resp){\r\n                            // //    删除成功\r\n                            // this.load();\r\n                            this.$message({\r\n                                message: '删除成功!',\r\n                                type: 'success'\r\n                            });\r\n                        }else {\r\n                            this.$message.error(\"删除失败\");\r\n                        }\r\n                    }).finally(()=>{\r\n                        this.load();\r\n                    });\r\n                }).catch(()=>{\r\n                    this.$message.info(\"取消删除\");\r\n                });\r\n\r\n            },\r\n\r\n            //批量删除\r\n            deleteByIds(){\r\n                // 将对象类型 直接转为 id数组\r\n                // [{},{},{}] => {1,2,3}\r\n                let studentIds =  this.multipleSelection.map(v => v.studentId);\r\n                this.request.post(\"/student/deletes\",studentIds).then(resp => {\r\n                    if (resp){\r\n                        //    删除成功\r\n                        this.load();\r\n                        this.$message({\r\n                            message: '批量删除成功!',\r\n                            type: 'success'\r\n                        });\r\n                    }else {\r\n                        this.$message.error(\"删除失败\");\r\n                    }\r\n                })\r\n            },\r\n\r\n            selectByCondition(){\r\n\r\n                this.request.get(\"/student/page3?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize+\"&studentId=\"+this.studentId+\"&name=\"+this.name+\"&email=\"+this.email).then(req => {\r\n                    this.tableData = req.data;\r\n                    this.total = req.total;\r\n                })\r\n                //  请求分页查询数据\r\n                // fetch(\"http://nxyi.ink:9090/user/page3?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize+\"&username=\"+this.username+\"&email=\"+this.email+\"&address=\"+this.address).then(res => res.json()).then(res =>{\r\n                //     console.log(res);\r\n                //     this.tableData = res.data;\r\n                //     // this.tableData = res.records;\r\n                //     this.total = res.total;\r\n                // })\r\n\r\n            },\r\n\r\n            //重置\r\n            reset(){\r\n                this.studentId = '';\r\n                this.name = '';\r\n                this.email = '';\r\n                this.load();\r\n            },\r\n\r\n            //新增按钮\r\n            handleAdd(){\r\n                this.dialogVisible = true;\r\n                this.formTitle = '添加学生';\r\n                this.form = {};\r\n            },\r\n            //新增数据表单创建方法 保存\r\n            addStudent(){\r\n\r\n                this.request.post(\"/student/add\",this.form).then(resp => {\r\n                    if (resp){\r\n                        //判断宿舍是否已满员\r\n                        if(resp.code === '123'){\r\n                            this.$message({\r\n                                message: resp.msg,\r\n                                type: 'warning'\r\n                            });\r\n                            return;\r\n                        }\r\n\r\n                        //    添加成功\r\n                        //    关闭窗口\r\n                        this.dialogVisible = false;\r\n                        this.form.avatar = resp.avatar;\r\n                        this.load();\r\n                        this.$message({\r\n                            message: '操作成功!',\r\n                            type: 'success'\r\n                        });\r\n                    }else {\r\n                        this.$message.error(\"保存失败\");\r\n                    }\r\n                });\r\n            },\r\n\r\n            //变换班级号\r\n            changeCName(classId){\r\n                this.form.classL = this.classN.find(item => item.classId === classId);\r\n                this.form.classId = this.form.classL.classId;\r\n                this.form.className = this.form.classL.label;\r\n            },\r\n            //变换宿舍号\r\n            changeDName(dormId){\r\n                this.form.dormL = this.dorm.find(item => item.dormId === dormId);\r\n                this.form.dormId = this.form.dormL.dormId;\r\n                this.form.dormName = this.form.dormL.label;\r\n            },\r\n\r\n//          头像上传成功\r\n            handleAvatarSuccess(res) {\r\n                this.form.avatar = res;\r\n            },\r\n            beforeAvatarUpload(file) {\r\n                const isJPG = file.type === 'image/jpeg' || 'image/png';\r\n                const isLt2M = file.size / 1024 / 1024 < 2;\r\n\r\n                if (!isJPG) {\r\n                    this.$message.error('上传头像图片只能是 JPG 格式!');\r\n                }\r\n                if (!isLt2M) {\r\n                    this.$message.error('上传头像图片大小不能超过 2MB!');\r\n                }\r\n                return isJPG && isLt2M;\r\n            },\r\n\r\n        //    导入\r\n            handleImportSuccess(){\r\n                this.$message.success(\"文件上传成功\");\r\n                this.load();\r\n            },\r\n\r\n        //    导出\r\n            exportStudents(){\r\n                window.open(\"http://nxyi.ink:9090/student/export\");\r\n            }\r\n\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    .headerBg{\r\n        background-color: #d7dde5 !important;\r\n    }\r\n    .avatar-uploader {\r\n        cursor: pointer;\r\n        position: relative;\r\n        overflow: hidden;\r\n        width: 50px;\r\n        height: 50px;\r\n        border-radius: 50%;\r\n        border: 1px dotted #95b5ea;\r\n        box-shadow: 3px 5px 5px rgba(135,206,235, 0.3);\r\n    }\r\n    .avatar-uploader {\r\n        border-color: #409EFF;\r\n    }\r\n    .avatar-uploader-icon {\r\n        font-size: 26px;\r\n        color: #8c939d;\r\n        width: 50px;\r\n        height: 50px;\r\n        line-height: 50px;\r\n        text-align: center;\r\n    }\r\n</style>"]},"metadata":{},"sourceType":"module"}
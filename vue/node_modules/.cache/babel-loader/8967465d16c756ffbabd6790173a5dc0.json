{"ast":null,"code":"import \"core-js/modules/es.function.name.js\";\nimport \"core-js/modules/es.array.map.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: \"Class\",\n  data: function data() {\n    return {\n      tableData: [],\n      multipleSelection: [],\n      className: '',\n      pageNum: 1,\n      pageSize: 5,\n      total: 0,\n      name: '',\n      isDelete: false,\n      headerBg: 'headerBg'\n    };\n  },\n  mounted: function mounted() {\n    this.load();\n  },\n  methods: {\n    load: function load() {\n      var _this = this;\n\n      this.request.get(\"/class/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize).then(function (req) {\n        _this.tableData = req.data;\n        _this.total = req.total;\n      });\n    },\n    //复选框选中后执行的方法\n    handleSelectionChange: function handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //分页\n    handleSizeChange: function handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n\n      if (this.username == null && this.email == null && this.address == null) {\n        this.load();\n      } else {\n        this.selectByCondition();\n      }\n    },\n    handleCurrentChange: function handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.selectByCondition();\n    },\n    //表格数据操作\n    handleEdit: function handleEdit(row) {\n      // this.form = row;\n      this.form = Object.assign({}, row); //将row拷贝到空对象中，解决没点确定 表中数据改变的问题\n\n      this.dialogVisible = true;\n      this.formTitle = '修改用户信息';\n    },\n    handleFileUploadSuccess: function handleFileUploadSuccess() {\n      this.load();\n    },\n    selectByCondition: function selectByCondition() {\n      var _this2 = this;\n\n      this.request.get(\"/class/page3?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize + \"&className=\" + this.className).then(function (req) {\n        _this2.tableData = req.data;\n        _this2.total = req.total;\n      });\n    },\n    //重置\n    reset: function reset() {\n      this.name = '';\n      this.load();\n    },\n    //文件下载\n    download: function download(url) {\n      window.open(url);\n    },\n    //数据删除\n    handleDelete: function handleDelete(id) {\n      var _this3 = this;\n\n      this.request(\"/file/delete/\" + id).then(function (resp) {\n        if (resp.code === '200') {\n          //    删除成功\n          _this3.$message.success(\"删除成功\");\n\n          _this3.load();\n        } else {\n          _this3.$message.error(\"删除失败\");\n        }\n      });\n    },\n    //批量删除\n    deleteByIds: function deleteByIds() {\n      var _this4 = this;\n\n      // 将对象类型 直接转为 id数组\n      // [{},{},{}] => {1,2,3}\n      var ids = this.multipleSelection.map(function (v) {\n        return v.id;\n      });\n      this.request.post(\"/file/deletes\", ids).then(function (resp) {\n        if (resp) {\n          //    删除成功\n          _this4.load();\n\n          _this4.$message({\n            message: '批量删除成功!',\n            type: 'success'\n          });\n        } else {\n          _this4.$message.error(\"删除失败\");\n        }\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoFA;AACAA,eADA;AAEAC,MAFA,kBAEA;AACA;AACAC,mBADA;AAEAC,2BAFA;AAGAC,mBAHA;AAIAC,gBAJA;AAKAC,iBALA;AAMAC,cANA;AAOAP,cAPA;AAQAQ,qBARA;AASAC;AATA;AAWA,GAdA;AAeAC,SAfA,qBAeA;AACA;AACA,GAjBA;AAmBAC;AAEAC,QAFA,kBAEA;AAAA;;AACA,6FACAC,IADA,CACA;AACA;AACA;AACA,OAJA;AAMA,KATA;AAUA;AACAC,yBAXA,iCAWAC,GAXA,EAWA;AACA;AACAC;AAEA,KAfA;AAgBA;AACAC,oBAjBA,4BAiBAX,QAjBA,EAiBA;AACA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;AAEA,KAzBA;AA0B
1'\n      }],\n      value: ''\n    };\n  },\n  mounted: function mounted() {\n    this.load();\n  },\n  methods: {\n    tableRowClassName: function tableRowClassName(_ref) {\n      var row = _ref.row,\n          rowIndex = _ref.rowIndex;\n\n      if (rowIndex === 1) {\n        return 'warning-row';\n      } else if (rowIndex === 3) {\n        return 'success-row';\n      }\n\n      return '';\n    },\n    //复选框选中后执行的方法\n    handleSelectionChange: function handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //分页\n    handleSizeChange: function handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n\n      if (this.studentId == null && this.name == null && this.email == null) {\n        this.load();\n      } else {\n        this.selectByCondition();\n      }\n    },\n    handleCurrentChange: function handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.selectByCondition();\n    },\n    //表格数据操作\n    handleEdit: function handleEdit(row) {\n      // this.form = row;\n      this.form = Object.assign({}, row); //将row拷贝到空对象中，解决没点确定 表中数据改变的问题\n\n      this.dialogVisible = true;\n      this.formTitle = '修改学生信息';\n    },\n    //数据删除\n    handleDelete: function handleDelete(studentId) {\n      var _this = this;\n\n      this.$confirm(\"此操作永久删除当前数据\", \"删除\", {\n        type: 'info'\n      }).then(function () {\n        _this.request(\"/student/delete/\" + studentId).then(function (resp) {\n          if (resp) {\n            // //    删除成功\n            // this.load();\n            _this.$message({\n              message: '删除成功!',\n              type: 'success'\n            });\n          } else {\n            _this.$message.error(\"删除失败\");\n          }\n        }).finally(function () {\n          _this.load();\n        });\n      }).catch(function () {\n        _this.$message.info(\"取消删除\");\n      });\n    },\n    //批量删除\n    deleteByIds: function deleteByIds() {\n      var _this2 = this;\n\n      // 将对象类型 直接转为 id数组\n      // [{},{},{}] => {1,2,3}\n      var studentIds = this.multipleSelection.map(function (v) {\n        return v.studentId;\n      });\n      this.request.post(\"/user/deletes\", studentIds).then(function (resp) {\n        if (resp) {\n          //    删除成功\n          _this2.load();\n\n          _this2.$message({\n            message: '批量删除成功!',\n            type: 'success'\n          });\n        } else {\n          _this2.$message.error(\"删除失败\");\n        }\n      });\n    },\n    load: function load() {\n      var _this3 = this;\n\n      this.request.get(\"/student/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize).then(function (req) {\n        _this3.tableData = req.data;\n        _this3.total = req.total;\n      }); // //  请求分页查询数据\n      // fetch(\"http://localhost:8081/user/page?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize).then(res => res.json()).then(res =>{\n      //     console.log(res);\n      //     this.tableData = res.data;\n      //     this.total = res.total;\n      // })\n    },\n    selectByCondition: function selectByCondition() {\n      var _this4 = this;\n\n      this.request.get(\"/student/page3?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize + \"&studentId=\" + this.studentId + \"&name=\" + this.name + \"&email=\" + this.email).then(function (req) {\n        _this4.tableData = req.data;\n        _this4.total = req.total;\n      }); //  请求分页查询数据\n      // fetch(\"http://localhost:8081/user/page3?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize+\"&username=\"+this.username+\"&email=\"+this.email+\"&address=\"+this.address).then(res => res.json()).then(res =>{\n      //     console.log(res);\n      //     this.tableData = res.data;\n      //     // this.tableData = res.records;\n      //     this.total = res.total;\n      // })\n    },\n    //重置\n    reset: function reset() {\n      this.studentId = '';\n      this.name = '';\n      this.email = '';\n      this.load();\n    },\n    //新增按钮\n    handleAdd: function handleAdd() {\n      this.dialogVisible = true;\n      this.formTitle = '添加学生';\n      this.form = {};\n    },\n    //新增数据表单创建方法 保存\n    addStudent: function addStudent() {\n      var _this5 = this;\n\n      this.request.post(\"/student/add\", this.form).then(function (resp) {\n        if (resp) {\n          //    添加成功\n          //    关闭窗口\n          console.log(_this5.form);\n          _this5.dialogVisible = false;\n          _this5.form.avatar = resp.avatar;\n          _this5.form.dormId = 'resp.dormId'; // this.form.classN.classId = resp.classId;\n\n          _this5.load();\n\n          _this5.$message({\n            message: '操作成功!',\n            type: 'success'\n          });\n        } else {\n          _this5.$message.error(\"保存失败\");\n        }\n      }); // console.log(this.tableData);\n      // this.$axios({\n      //     method: \"post\",\n      //     url: \"http://localhost:8081/user/add\",\n      //     data: this.tableData\n      // }).then(function (resp) {\n      //     if (resp.data === \"success\"){\n      //         //    添加成功\n      //         //    关闭窗口\n      //         this.dialogVisible = false;\n      //\n      //         //    重新查询数据\n      //         this.load();\n      //         //    弹出消息提示\n      //         this.$message({\n      //             message: '添加成功!',\n      //             type: 'success'\n      //         })\n      //\n      //     }\n      // })\n    },\n    //          头像上传成功\n    handleAvatarSuccess: function handleAvatarSuccess(res) {\n      this.form.avatar = res;\n    },\n    beforeAvatarUpload: function beforeAvatarUpload(file) {\n      var isJPG = file.type === 'image/jpeg' || 'image/png';\n      var isLt2M = file.size / 1024 / 1024 < 2;\n\n      if (!isJPG) {\n        this.$message.error('上传头像图片只能是 JPG 格式!');\n      }\n\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!');\n      }\n\n      return isJPG && isLt2M;\n    },\n    //    导入\n    handleImportSuccess: function handleImportSuccess() {\n      this.$message.success(\"文件上传成功\");\n      this.load();\n    },\n    //    导出\n    exportStudents: function exportStudents() {\n      window.open(\"http://localhost:8081/student/export\");\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4KA;AACAA,qBADA;AAEAC;AACAC,uBADA;AAEAC;AAFA,GAFA;AAMAC,MANA,kBAMA;AACA;AACA;AACAC,2BAFA;AAGAC,mBAHA;AAGA;AACAC,cAJA;AAIA;AACAC,gBALA;AAMAC,kBANA;AAOAC,0BAPA;AAQAC,cARA;AASAC,mBATA;AAUAC,mBAVA;AAWAb,cAXA;AAYAc,gBAZA;AAaAC,mBAbA;AAcAC,kBAdA;AAeAC,gBAfA;AAgBAC,eAhBA;AAiBAC,eAjBA;AAkBAC,kBAlBA;AAmBAC,iBAnBA;AAoBAC,gBApBA;AAqBAC,0BArBA;AAsBAC;AACAP,qBADA;AAEAQ;AAFA,SAGA;AACAR,qBADA;AAEAQ;AAFA,OAHA,EAMA;AACAR,qBADA;AAEAQ;AAFA,OANA,EASA;AACAR,qBADA;AAEAQ;AAFA,OATA,CAtBA;AAmCAC;AACAC,0BADA;AAEAF;AAFA,SAGA;AACAE,0BADA;AAEAF;AAFA,OAHA,EAMA;AACAE,0BADA;AAEAF;AAFA,OANA,EASA;AACAE,0BADA;AAEAF;AAFA,OATA,CAnCA;AAgDAG;AAhDA;AAkDA,GAzDA;AA2DAC,SA3DA,qBA2DA;AACA;AACA,GA7DA;AA+DAC;AACAC,qBADA,mCACA;AAAA;AAAA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;;AACA;AACA,KARA;AAUA;AACAC,yBAXA,iCAWAC,GAXA,EAWA;AACA;AACAC;AAEA,KAfA;AAeA;AACAC,oBAhBA,4BAgBA1B,QAhBA,EAgBA;AACA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;AAEA,KAxBA;AAyBA2B,uBAzBA,+BAyBA5B,OAzBA,EAyBA;AACA;AACA;AACA,KA5BA;AA8BA;AACA6B,cA/BA,sBA+BAC,GA/BA,EA+BA;AACA;AACA,yCAFA,CAEA;;AACA;AACA;AACA,KApCA;AAsCA;AACAC,gBAvCA,wBAuCA1B,SAvCA,EAuCA;AAAA;;AACA;AACA2B;AADA,SAEAC,IAFA,CAEA;AACA;AACA;AACA;AACA;AACA;AACAC,8BADA;AAEAF;AAFA;AAIA,WAPA,MAOA;AACA;AACA;AACA,SAXA,EAWAG,OAXA,CAWA;AACA;AACA,SAbA;AAcA,OAjBA,EAiBAC,KAjBA,CAiBA;AACA;AACA,OAnBA;AAqBA,KA7DA;AA+DA;AACAC,eAhEA,yBAgEA;AAAA;;AACA;AACA;AACA;AAAA;AAAA;AACA;AACA;AACA;AACA;;AACA;AACAH,8BADA;AAEAF;AAFA;AAIA,SAPA,MAOA;AACA;AACA;AACA,OAXA;AAYA,KAhFA;AAiFAM,QAjFA,kBAiFA;AAAA;;AACA,+FACAL,IADA,CACA;AACA;AACA;AACA,OAJA,EADA,CAOA;AACA;AACA;AACA;
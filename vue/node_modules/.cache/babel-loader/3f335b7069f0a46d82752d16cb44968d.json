{"ast":null,"code":"import \"core-js/modules/es.function.name.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/es.array.map.js\";\nimport \"core-js/modules/es.array.find.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: \"StudentView\",\n  props: {\n    isCollapse: Boolean,\n    logoTextShow: Boolean\n  },\n  data: function data() {\n    return {\n      // 多选框数据\n      multipleSelection: [],\n      tableData: [],\n      //表单数据\n      total: 0,\n      //总条数\n      pageNum: 1,\n      pageSize: 10,\n      dialogVisible: false,\n      form: {},\n      dormL: {},\n      classL: {},\n      formTitle: '',\n      studentId: '',\n      name: '',\n      gender: '',\n      className: '',\n      dormName: '',\n      dormId: '',\n      phone: '',\n      email: '',\n      birthday: '',\n      address: '',\n      avatar: '',\n      headerBg: 'headerBg',\n      dorm: [{\n        dormId: '101',\n        label: '公寓01-101'\n      }, {\n        dormId: '102',\n        label: '公寓02-202'\n      }, {\n        dormId: '103',\n        label: '公寓03-303'\n      }, {\n        dormId: '104',\n        label: '公寓04-404'\n      }],\n      classN: [{\n        classId: '2019101',\n        label: '磨角193'\n      }]\n    };\n  },\n  mounted: function mounted() {\n    this.load();\n  },\n  methods: {\n    tableRowClassName: function tableRowClassName(_ref) {\n      var row = _ref.row,\n          rowIndex = _ref.rowIndex;\n\n      if (rowIndex === 1) {\n        return 'warning-row';\n      } else if (rowIndex === 3) {\n        return 'success-row';\n      }\n\n      return '';\n    },\n    load: function load() {\n      var _this = this;\n\n      this.request.get(\"/student/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize).then(function (req) {\n        _this.tableData = req.data;\n        _this.total = req.total;\n      });\n      this.request.get(\"/class/\").then(function (req) {\n        var i = 0; // for (i = 0; i < req.length; i++) {\n        //     this.classN[i].classId = req[i].classId;\n        //     this.classN[i].label = req[i].className;\n        // }\n\n        _this.classN = req;\n        console.log(req.length);\n        console.log(_this.classN[0].classId);\n      });\n    },\n    //复选框选中后执行的方法\n    handleSelectionChange: function handleSelectionChange(val) {\n      this.multipleSelection = val;\n      console.log(this.multipleSelection);\n    },\n    //分页\n    handleSizeChange: function handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n\n      if (this.studentId == null && this.name == null && this.email == null) {\n        this.load();\n      } else {\n        this.selectByCondition();\n      }\n    },\n    handleCurrentChange: function handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.selectByCondition();\n    },\n    //表格数据操作\n    handleEdit: function handleEdit(row) {\n      // this.form = row;\n      this.form = Object.assign({}, row); //将row拷贝到空对象中，解决没点确定 表中数据改变的问题\n\n      this.dialogVisible = true;\n      this.formTitle = '修改学生信息';\n    },\n    //数据删除\n    handleDelete: function handleDelete(studentId) {\n      var _this2 = this;\n\n      this.$confirm(\"此操作永久删除当前数据\", \"删除\", {\n        type: 'info'\n      }).then(function () {\n        _this2.request(\"/student/delete/\" + studentId).then(function (resp) {\n          if (resp) {\n            // //    删除成功\n            // this.load();\n            _this2.$message({\n              message: '删除成功!',\n              type: 'success'\n            });\n          } else {\n            _this2.$message.error(\"删除失败\");\n          }\n        }).finally(function () {\n          _this2.load();\n        });\n      }).catch(function () {\n        _this2.$message.info(\"取消删除\");\n      });\n    },\n    //批量删除\n    deleteByIds: function deleteByIds() {\n      var _this3 = this;\n\n      // 将对象类型 直接转为 id数组\n      // [{},{},{}] => {1,2,3}\n      var studentIds = this.multipleSelection.map(function (v) {\n        return v.studentId;\n      });\n      this.request.post(\"/student/deletes\", studentIds).then(function (resp) {\n        if (resp) {\n          //    删除成功\n          _this3.load();\n\n          _this3.$message({\n            message: '批量删除成功!',\n            type: 'success'\n          });\n        } else {\n          _this3.$message.error(\"删除失败\");\n        }\n      });\n    },\n    selectByCondition: function selectByCondition() {\n      var _this4 = this;\n\n      this.request.get(\"/student/page3?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize + \"&studentId=\" + this.studentId + \"&name=\" + this.name + \"&email=\" + this.email).then(function (req) {\n        _this4.tableData = req.data;\n        _this4.total = req.total;\n      }); //  请求分页查询数据\n      // fetch(\"http://localhost:8081/user/page3?pageNum=\"+this.pageNum+\"&pageSize=\"+this.pageSize+\"&username=\"+this.username+\"&email=\"+this.email+\"&address=\"+this.address).then(res => res.json()).then(res =>{\n      //     console.log(res);\n      //     this.tableData = res.data;\n      //     // this.tableData = res.records;\n      //     this.total = res.total;\n      // })\n    },\n    //重置\n    reset: function reset() {\n      this.studentId = '';\n      this.name = '';\n      this.email = '';\n      this.load();\n    },\n    //新增按钮\n    handleAdd: function handleAdd() {\n      this.dialogVisible = true;\n      this.formTitle = '添加学生';\n      this.form = {};\n    },\n    //新增数据表单创建方法 保存\n    addStudent: function addStudent() {\n      var _this5 = this;\n\n      this.request.post(\"/student/add\", this.form).then(function (resp) {\n        if (resp) {\n          //判断宿舍是否已满员\n          if (resp.code === '123') {\n            _this5.$message({\n              message: resp.msg,\n              type: 'warning'\n            });\n\n            return;\n          } //    添加成功\n          //    关闭窗口\n\n\n          _this5.dialogVisible = false;\n          _this5.form.avatar = resp.avatar;\n\n          _this5.load();\n\n          _this5.$message({\n            message: '操作成功!',\n            type: 'success'\n          });\n        } else {\n          _this5.$message.error(\"保存失败\");\n        }\n      });\n    },\n    //变换班级号\n    changeCName: function changeCName(classId) {\n      this.form.classL = this.classN.find(function (item) {\n        return item.classId === classId;\n      });\n      this.form.classId = this.form.classL.classId;\n      this.form.className = this.form.classL.label;\n    },\n    //变换宿舍号\n    changeDName: function changeDName(dormId) {\n      this.form.dormL = this.dorm.find(function (item) {\n        return item.dormId === dormId;\n      });\n      this.form.dormId = this.form.dormL.dormId;\n      this.form.dormName = this.form.dormL.label;\n    },\n    //          头像上传成功\n    handleAvatarSuccess: function handleAvatarSuccess(res) {\n      this.form.avatar = res;\n    },\n    beforeAvatarUpload: function beforeAvatarUpload(file) {\n      var isJPG = file.type === 'image/jpeg' || 'image/png';\n      var isLt2M = file.size / 1024 / 1024 < 2;\n\n      if (!isJPG) {\n        this.$message.error('上传头像图片只能是 JPG 格式!');\n      }\n\n      if (!isLt2M) {\n        this.$message.error('上传头像图片大小不能超过 2MB!');\n      }\n\n      return isJPG && isLt2M;\n    },\n    //    导入\n    handleImportSuccess: function handleImportSuccess() {\n      this.$message.success(\"文件上传成功\");\n      this.load();\n    },\n    //    导出\n    exportStudents: function exportStudents() {\n      window.open(\"http://localhost:8081/student/export\");\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA4KA;AACAA,qBADA;AAEAC;AACAC,uBADA;AAEAC;AAFA,GAFA;AAMAC,MANA,kBAMA;AACA;AACA;AACAC,2BAFA;AAGAC,mBAHA;AAGA;AACAC,cAJA;AAIA;AACAC,gBALA;AAMAC,kBANA;AAOAC,0BAPA;AAQAC,cARA;AASAC,eATA;AAUAC,gBAVA;AAWAC,mBAXA;AAYAC,mBAZA;AAaAf,cAbA;AAcAgB,gBAdA;AAeAC,mBAfA;AAgBAC,kBAhBA;AAiBAC,gBAjBA;AAkBAC,eAlBA;AAmBAC,eAnBA;AAoBAC,kBApBA;AAqBAC,iBArBA;AAsBAC,gBAtBA;AAuBAC,0BAvBA;AAwBAC;AACAP,qBADA;AAEAQ;AAFA,SAGA;AACAR,qBADA;AAEAQ;AAFA,OAHA,EAMA;AACAR,qBADA;AAEAQ;AAFA,OANA,EASA;AACAR,qBADA;AAEAQ;AAFA,OATA,CAxBA;AAqCAC;AACAC,0BADA;AAEAF;AAFA;AArCA;AA0CA,GAjDA;AAmDAG,SAnDA,qBAmDA;AACA;AACA,GArDA;AAuDAC;AAEAC,qBAFA,mCAEA;AAAA;AAAA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;;AACA;AACA,KATA;AAUAC,QAVA,kBAUA;AAAA;;AACA,+FACAC,IADA,CACA;AACA;AACA;AACA,OAJA;AAKA;AACA,kBADA,CAEA;AACA;AACA;AACA;;AACA;AACAC;AACAA;AACA,OATA;AAUA,KA1BA;AA4BA;AACAC,yBA7BA,iCA6BAC,GA7BA,EA6BA;AACA;AACAF;AAEA,KAjCA;AAiCA;AACAG,oBAlCA,4BAkCA7B,QAlCA,EAkCA;AACA;;AACA;AACA;AACA,OAFA,MAEA;AACA;AACA;AAEA,KA1CA;AA2CA8B,uBA3CA,+BA2CA/B,OA3CA,EA2CA;AACA;AACA;AACA,KA9CA;AAgDA;AACAgC,cAjDA,sBAiDAC,GAjDA,EAiDA;AACA;AACA,yCAFA,CAEA;;AACA;AACA;AACA,KAtDA;AAwDA;AACAC,gBAzDA,wBAyDA3B,SAzDA,EAyDA;AAAA;;AACA;AACA4B;AADA,SAEAT,IAFA,CAEA;AACA;AACA;AACA;AACA;AACA;AACAU,8BADA;AAEAD;AAFA;AAIA,WAPA,MAOA;AACA;AACA;AACA,SAXA,EAWAE,OAXA,CAWA;AACA;AACA,SAbA;AAcA,OAjBA,EAiBAC,KAjBA,CAiBA;AACA;AACA,OAnBA;AAqBA,KA/EA;AAiFA;AACAC,eAlFA,yBAkFA;AAAA;;AACA;AACA;AACA;AAAA;AAAA;AACA;AACA;AACA;AACA;;AACA;AACAH,8BADA;AAEAD;AAFA;AAIA,SAPA,MAOA;AACA;AACA;AACA,OAXA;AAYA,KAlGA;AAoGAK,qBApGA,+BAoGA;AAAA;;AAEA;AACA;AACA;AACA,OAHA,EAFA,CA
his.isCollapse) {\n        //收缩\n        this.sideWidth = 64;\n        this.logoTextShow = false;\n        this.collapseBtnClass = 'el-icon-s-unfold';\n      } else {\n        //展开\n        this.sideWidth = 200;\n        this.logoTextShow = true;\n        this.collapseBtnClass = 'el-icon-s-fold';\n      }\n    },\n    //分页\n    handleSizeChange: function handleSizeChange(pageSize) {\n      this.pageSize = pageSize;\n      this.load();\n    },\n    handleCurrentChange: function handleCurrentChange(pageNum) {\n      this.pageNum = pageNum;\n      this.load();\n    },\n    //表格数据操作\n    handleEdit: function handleEdit(index, row) {\n      console.log(index, row);\n    },\n    handleDelete: function handleDelete(index, row) {\n      console.log(index, row);\n    },\n    load: function load() {\n      var _this = this;\n\n      // //  请求分页查询数据\n      fetch(\"http://localhost:8081/user/page?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize).then(function (res) {\n        return res.json();\n      }).then(function (res) {\n        console.log(res);\n        _this.tableData = res.data;\n        _this.total = res.total;\n      });\n    },\n    selectByCondition: function selectByCondition() {\n      var _this2 = this;\n\n      //  请求分页查询数据\n      fetch(\"http://localhost:8081/user/page2?pageNum=\" + this.pageNum + \"&pageSize=\" + this.pageSize + \"&username=\" + this.username + \"&email=\" + this.email + \"&address=\" + this.address).then(function (res) {\n        return res.json();\n      }).then(function (res) {\n        console.log(res);\n        _this2.tableData = res.data;\n        _this2.total = res.total;\n      });\n    },\n    //新增数据表单创建方法\n    addBrand: function addBrand() {\n      console.log(this.brand);\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAuMA;A